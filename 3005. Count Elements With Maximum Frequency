class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map<int, int> freq ; 
        int maxifreq = 0 ; 

        for(auto num : nums){
            freq[num]++ ; 
            maxifreq = max(maxifreq, freq[num]) ; 
        }

        int total = 0 ; 
        for(auto p : freq){
            if(p.second == maxifreq) {
                total += p.second ; 
            }
        }
        return total ; 
    }
};


Time Complexity : O(2N)  ; 
